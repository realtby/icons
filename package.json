{
  "name": "@realtby/icons",
  "description": "realt.by icons library",
  "version": "1.0.0",
  "main": "./dist/icons.js",
  "module": "./dist/icons.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle --jsx React.createElement --jsxImportSource react -f esm,cjs",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts,.tsx,.js,.cjs,.json --fix --cache --max-warnings=0",
    "release": "release-it",
    "pre-release": "release-it --no-git",
    "size-limit": "size-limit",
    "type-check": "tsc --pretty --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "postinstall": "husky install",
    "husky:pre-push": "npm run type-check"
  },
  "peerDependencies": {
    "classnames": "^2.x",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@realtby/codestyle": "^0.1.13",
    "@size-limit/preset-big-lib": "^7.0.3",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@types/react": "^17.0.36",
    "auto-changelog": "^2.3.0",
    "babel-loader": "^8.2.3",
    "classnames": "^2.3.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "microbundle": "^0.14.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "release-it": "^14.11.8",
    "size-limit": "^7.0.3",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": "~14.17",
    "npm": "~6.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realtby/icons.git"
  },
  "keywords": [
    "icons",
    "svg",
    "lib",
    "library",
    "react"
  ],
  "author": "realt.by team <admin@realt.by> (https://github.com/realtby)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/realtby/icons/issues"
  },
  "homepage": "https://github.com/realtby/icons#readme"
}
